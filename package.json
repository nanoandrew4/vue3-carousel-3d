{
  "name": "@nanoandrew4/vue3-carousel-3d",
  "version": "1.0.2",
  "description": "Beautiful, flexible and touch supported 3D Carousel for Vue.js",
  "type": "module",
  "main": "./dist/vue3-carousel-3d.umd.cjs",
  "module": "./dist/vue3-carousel-3d.js",
  "types": "./dist/index.d.ts",
  "export": {
    ".": {
      "import": "./dist/vue3-carousel-3d.js",
      "require": "./dist/vue3-carousel-3d.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "mod-update": "rm -rf ./node_modules && rm -f package-lock.json && npm i",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "coverage": "vitest run --coverage",
    "test:unit": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nanoandrew4/vue3-carousel-3d.git"
  },
  "keywords": [
    "Vue3",
    "carousel",
    "carousel 3d",
    "slider"
  ],
  "author": "Vladimir Bujanovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nanoandrew4/vue3-carousel-3d/issues"
  },
  "homepage": "https://github.com/nanoandrew4/vue3-carousel-3d#readme",
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.11.25",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^8.7.1",
    "jest-serializer-vue-tjw": "^3.20.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  },
  "jestSerializer": {
    "removeComments": true
  }
}
